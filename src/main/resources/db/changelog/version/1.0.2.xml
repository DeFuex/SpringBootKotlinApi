<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <changeSet id="2018-05-11 10:56" author="35_bahones_61">
        <createSequence sequenceName="seq_zone_category" incrementBy="50"/>
        <createTable tableName="zone_category">
            <column name="id" type="int8">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="zone_id" type="int8">
                <constraints nullable="false"/>
            </column>
            <column name="category_id" type="int8" defaultValue="0"/>
            <column name="deleted" type="boolean" defaultValue="false"/>
            <column name="created_date" type="datetime"/>
            <column name="last_modified_date" type="datetime"/>
        </createTable>
        <addForeignKeyConstraint baseTableName="zone_category" baseColumnNames="zone_id" constraintName="fk_zone_category_zone_id"
            referencedTableName="zone"
            referencedColumnNames="id"/>
    </changeSet>

</databaseChangeLog>